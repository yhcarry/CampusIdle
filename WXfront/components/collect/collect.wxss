/* components/collect/collect.wxss */
.note{
  font-size: 20px;
  margin-top: 20px;
  text-align: center;
  color: #949494;
}
.thumb {
    display:flex;
    flex-direction: column;
    width:30%px;
    height:110px;
    justify-content: center;
    margin-right: 15rpx;
}

.info{
    display:flex;
    flex-direction: column;
    width:35%;
    justify-content: space-around;
    font-size: 30rpx;
}

.shop-title{
    font-weight: bold;
} 
.image{
    height:15px;
    width:15px;
    justify-content: center;
    margin-right: 15rpx;
}
.item {
    margin-bottom: 200rpx;
    background-color: #fff;
    padding: 20rpx 8rpx;
    padding-right: 0;
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    height: 120rpx;
  }
  
  /* 除删除按钮之外的其他部分 */
  .item-left {
    display: flex;
    justify-content: start;
    align-items: center;
  
    /* 关键代码 start */
    width: 100%;
    margin-left: -140rpx;
    transform: translateX(140rpx);
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
    -webkit-transform: translateX(140rpx);
    /* 关键代码 end */
  }
  
  .item-left .icon-check {
    width: 80rpx;
    height: 80rpx;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  
  .icon-check image {
    width: 60rpx;
    height: 45rpx;
    display: block;
  }
  

  /* 删除按钮 */
  .delete {
    height: 100%;
    width: 220rpx;
    background-color: rgb(241, 81, 81);
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 36rpx;
    color: #fff;
    /* 关键代码 start */
    transform: translateX(150rpx);
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
    -webkit-transform: translateX(150rpx);
    /* 关键代码 end */
  }
  
  /* 关键代码 start --> 向左滑动 */
  .touch-move-active .item-left,
  .touch-move-active .delete {
    -webkit-transform: translateX(0);
    transform: translateX(0);
  }
  /* 关键代码 end */

