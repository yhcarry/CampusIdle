.card {
  width: 100%;
  height: 100px;
  display: flex;
  flex-direction: row;
  margin-bottom: 1px;
}

.card-img {
  height: 100px;
  width: 100px;
  padding: 7px 7px;
}

.card-img>image {
  width: 86px;
  height: 86px;
}

.card-text {
  height: 100px;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.card-text_ {
  width: 100%;
  height: 60px;
  display: flex;
  flex-direction: row;
}

.card-text_>text {
  width: 80%;
  padding-left: 20rpx;
  color: #444;
  font-weight: 600;
  font-size: 12pt;
}

.card-text_>view {
  padding-right: 20rpx;
  width: 20%;
  text-align: right;
}

.card-text_>view>view {
  color: #444;
  font-weight: 600;
  margin-bottom: 10rpx;
}

.card-text_>view>text {
  font-size: 9pt;
  color: #999;
}

.card-button {
  width: 100%;
  line-height: 50px;
  display: flex;
  flex-direction: row;
  padding-left: 30rpx;
}

.card-button>view {
  background-color: #fff;
  line-height: 25px;
  width: 30%;
  margin-left: 5rpx;
  text-align: center;
  border-radius: 15px;
  border:1px solid#d6d6d6;
  color: #444;
  font-size: 9pt;
}
.shop{
  margin-top: 10px;
  width:100%;
}
 .shop-item{
  display: flex;
  flex-direction: row;
  padding: 15rpx;
  height:110px;
  width:86%;
  padding-left: 5%;
  padding-right: 5%;
  border: 1rpx solid #efefef;
  border-radius: 8rpx;

  margin:15rpx;
  border-radius: 5%;
  box-shadow: 1rpx 1rpx 15rpx #dddddd;
}
.card-button-one{
  font-size: 11pt;
  margin-right:10px;
  letter-spacing:0;
}

