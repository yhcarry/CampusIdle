<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{ active }}" bind:change="onChange" active-color='#13A7A0'>
  <!-- <van-tabbar-item wx:for="{{list}}" wx:key="index" info="{{item.info?item.info:''}}"> -->
  <van-tabbar-item wx:for="{{list}}" wx:key="index">
    <image slot="icon" src="{{ item.iconPath }}" mode="aspectFit" style="width: 30px; height: 30px;" />
    <image slot="icon-active" src="{{ item.selectedIconPath }}" mode="aspectFit" style="width: 30px; height: 30px;" />
    {{item.text}}
  </van-tabbar-item>
</van-tabbar>